#include<stdio.h>
#include<conio.h>
main()
{

	int a ,b ,c ;
	clrscr();

	printf("value of a is :");
	scanf("%d",&a);
	printf("value of b is :");
	scanf("%d",&b);
	printf("value of c is :");
	scanf("%d",&c);

	if(a==b && b==c && a==c )
	   {
	      printf("all are same....");
	   }
	else
	{
	   if (a==b)
	   {
	     printf("a and b are same ...");
	   }
	   else if (b==c)
	   {
	     printf("b and c are same ....");
	   }
	   else if (c==a)
	   {
	     printf("a and c are same ....");
	   }
     else
     {
	if(a>b)
	{
	 if(b>c)
	 {
	  printf("b is maximum");
	 }
	 else
	 {
	  printf("c is maximum");
	 }
	}

	 else
	 {
	  if (b>c)
	     {
	       printf("b is maximum");
	     }
	 else
	     {
	       printf("c is maximum");
	     }
	 }
       }
     }
     getch();
}